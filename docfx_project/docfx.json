{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "/SquigglesCommon4X.csproj"
          ],
          "src": "../"
        }
      ],
      "disableDefaultFilter": false,
      "filter": "sc4x_utility_filter.yml",
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "EnumSortOrder": "declaringOrder",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseUrl": "https://queenofsquiggles.github.io/Squiggles-Core-4X/",
      "priority": 0.5,
      "changefreq": "daily"
    }
  }
}